{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/weather-subscription-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/weather-subscription-service/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/weather-subscription-service",
  "projects": {
    "email": {
      "type": "application",
      "root": "apps/email",
      "entryFile": "main",
      "sourceRoot": "apps/email/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "notification": {
      "type": "application",
      "root": "apps/notification",
      "entryFile": "main",
      "sourceRoot": "apps/notification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification/tsconfig.app.json"
      }
    },
    "subscription": {
      "type": "application",
      "root": "apps/subscription",
      "entryFile": "main",
      "sourceRoot": "apps/subscription/src",
      "compilerOptions": {
        "tsConfigPath": "apps/subscription/tsconfig.app.json"
      }
    },
    "weather": {
      "type": "application",
      "root": "apps/weather",
      "entryFile": "main",
      "sourceRoot": "apps/weather/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weather/tsconfig.app.json"
      }
    },
    "weather-subscription-service": {
      "type": "application",
      "root": "apps/weather-subscription-service",
      "entryFile": "main",
      "sourceRoot": "apps/weather-subscription-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weather-subscription-service/tsconfig.app.json"
      }
    }
  }
}